{
  "name": "mamka",
  "version": "0.0.0-semantic-released",
  "scripts": {
    "postinstall": "webdriver-manager update",
    "validate": "npm-run-all --parallel validate:* lint",
    "validate:dev": "webpack-validator config/webpack.dev.js",
    "validate:test": "webpack-validator config/webpack.test.js",
    "validate:prod": "webpack-validator config/webpack.prod.js",
    "lint": "eslint config",
    "serve": "node server.js",
    "serve:dev": "browser-sync start --config config/browser-sync.js",
    "serve:webdriver": "webdriver-manager start",
    "build": "webpack --config config/webpack.prod.js",
    "build:dev": "webpack --config config/webpack.dev.js",
    "test": "npm-run-all test:unit coverage:*",
    "test:e2e": "protractor config/protractor.prod.js",
    "test:e2e:dev": "protractor config/protractor.dev.js",
    "test:unit": "karma start config/karma.prod.js",
    "test:unit:dev": "karma start config/karma.dev.js",
    "coverage:check": "istanbul check-coverage",
    "coverage:report": "cat ./reports/coverage/lcov.info | coveralls"
  },
  "dependencies": {
    "angular": "1.5.7",
    "angular-mocks": "1.5.7",
    "autoprefixer": "6.3.7",
    "babel-core": "6.10.4",
    "babel-eslint": "6.1.2",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.9.0",
    "clean-webpack-plugin": "0.1.9",
    "compression": "1.6.2",
    "css-loader": "0.23.1",
    "eslint": "3.0.1",
    "eslint-loader": "1.4.1",
    "exports-loader": "0.6.3",
    "express": "4.14.0",
    "express-history-api-fallback": "2.0.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.22.0",
    "imports-loader": "0.6.5",
    "jade": "1.11.0",
    "jade-html-loader": "0.0.3",
    "jade-loader": "0.8.0",
    "lodash": "4.13.1",
    "ng-annotate-loader": "0.1.0",
    "ng-cache-loader": "0.0.16",
    "npm-run-all": "2.3.0",
    "postcss-loader": "0.9.1",
    "style-loader": "0.13.1",
    "stylint": "1.5.6",
    "stylint-loader": "1.0.0",
    "stylus": "0.54.5",
    "stylus-loader": "2.1.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.1",
    "webpack-merge": "0.14.0",
    "webpack-validator": "2.2.2"
  },
  "devDependencies": {
    "babel-plugin-__coverage__": "11.0.0",
    "babel-register": "6.9.0",
    "browser-sync": "2.13.0",
    "coveralls": "2.11.9",
    "istanbul": "0.4.4",
    "jasmine": "2.4.1",
    "karma": "1.1.1",
    "karma-chrome-launcher": "1.0.1",
    "karma-coverage": "1.1.0",
    "karma-firefox-launcher": "1.0.0",
    "karma-html-reporter": "0.2.7",
    "karma-jasmine": "1.0.2",
    "karma-mocha-reporter": "2.0.4",
    "karma-webpack": "1.7.0",
    "protractor": "3.3.0"
  },
  "engines": {
    "node": ">= 6",
    "npm": ">= 3"
  }
}
